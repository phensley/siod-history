X/* This illustrates calling the main program entry points and enabling our
X   own example subrs */
X
XLISP our_gc_mark();
Xvoid our_gc_free();
Xvoid our_print();
XLISP our_readm();
XLISP our_eval();
X
Xmain(argc,argv)
X int argc; char **argv;
X{long gc_kind;
X print_welcome();
X process_cla(argc,argv,1);
X set_gc_hooks(NULL,NULL,NULL,NULL,&gc_kind);
X print_hs_1();
X init_storage();
