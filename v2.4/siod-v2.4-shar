XLISP save_forms(fname,forms,how)
X     LISP fname,forms,how;
X{char *cname,*chow;
X LISP l;
X FILE *f;
X if NSYMBOLP(fname) err("filename not a symbol",fname);
X cname = PNAME(fname);
X if EQ(how,NIL) chow = "w";
X else if EQ(how,cintern("a")) chow = "a";
X else err("bad argument to save-forms",how);
X put_st((*chow == 'a') ? "appending" : "saving");
X put_st(" forms to ");
X put_st(cname);
X put_st("\n");
X f = fopen_care(cname,chow);
X for(l=forms;NNULLP(l);l=cdr(l))
X   {lprin1f(car(l),f);
X    putc('\n',f);}
X fclose_care(f);
X put_st("done.\n");
X return(truth);}
