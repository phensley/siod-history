Xint f_getc(f)
X     FILE *f;
X{long iflag,dflag;
X int c;
X iflag = no_interrupt(1);
X dflag = interrupt_differed;
X c = getc(f);
X#ifdef VMS
X if ((dflag == 0) & interrupt_differed & (f == stdin))
X   while((c != 0) & (c != EOF)) c = getc(f);
X#endif
X no_interrupt(iflag);
X return(c);}
X
Xvoid f_ungetc(c,f)
X     int c; FILE *f;
X{ungetc(c,f);}
X
X int
Xflush_ws(f,eoferr)
X struct gen_readio *f;
X char *eoferr;
X{int c,commentp;
X commentp = 0;
X while(1)
X   {c = GETC_FCN(f);
X    if (c == EOF) if (eoferr) err(eoferr,NIL); else return(c);
X    if (commentp) {if (c == '\n') commentp = 0;}
X    else if (c == ';') commentp = 1;
X    else if (!isspace(c)) return(c);}}
X
XLISP lreadf(f)
X     FILE *f;
X{return(gen_read(f_getc,f_ungetc,f));}
X
XLISP readtl(f)
X  struct gen_readio *f;
X{int c;
X c = flush_ws(f,(char *)NULL);
X if (c == EOF) return(eof_val);
X UNGETC_FCN(c,f);
X return(lreadr(f));}
X
XLISP gen_read(f1,f2,x)
X     int (*f1)();
X     void (*f2)();
X     char *x;
X{struct gen_readio f;
X f.getc_fcn = f1;
X f.ungetc_fcn = f2;
X f.cb_argument = x;
X return(readtl(&f));}
