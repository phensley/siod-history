XFILE *fopen_care(name,how)
X     char *name,*how;
X{FILE *f;
X LISP sym;
X long flag;
X sym = symcons(0,NIL);
X open_files = cons(sym,open_files);
X flag = no_interrupt(1);
X f = fopen(name,how);
X if (!f)
X   {perror(name);
X    err("could not open file",NIL);}
X PNAME(sym) = (char *) f;
X no_interrupt(flag);
X return(f);}
X
XLISP fclose_dq(f,l)
X     FILE *f;
X     LISP l;
X{FILE *p;
X if NULLP(l) return(l);
X if (PNAME(CAR(l)) == (char *) f) return(CDR(l));
X CDR(l) = fclose_dq(f,CDR(l));
X return(l);}
X
X
Xfclose_care(f)
X     FILE *f;
X{long flag;
X LISP l;
X flag = no_interrupt(1);
X fclose(f);
X open_files = fclose_dq(f,open_files);
X no_interrupt(flag);}
