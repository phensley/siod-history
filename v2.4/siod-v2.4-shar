X/* This is cfib, for compiled fib. Test to see what the overhead
X   of interpretation actually is in a given implementation 
X */
X
XLISP my_one;
XLISP my_two;
X
X/*   (define (standard-fib x)
X       (if (< x 2)
X         x
X         (+ (standard-fib (- x 1))
X	    (standard-fib (- x 2)))))  
X*/
X
XLISP cfib(x)
X     LISP x;
X{if NNULLP(lessp(x,my_two))
X   return(x);
X else
X   return(plus(cfib(difference(x,my_one)),
X	       cfib(difference(x,my_two))));}
X
X
X#ifdef vms
X#include <descrip.h>
X#include <ssdef.h>
XLISP sys_edit(fname)
X LISP fname;
X{struct dsc$descriptor_s d;
X long iflag;
X if NTYPEP(fname,tc_symbol) err("filename not a symbol",fname);
X d.dsc$b_dtype = DSC$K_DTYPE_T;
X d.dsc$b_class = DSC$K_CLASS_S;
X d.dsc$w_length = strlen(PNAME(fname));
X d.dsc$a_pointer = PNAME(fname);
X iflag = no_interrupt(1);
X edt$edit(&d);
X no_interrupt(iflag);
X return(fname);}
X
XLISP vms_debug(v)
X     LISP v;
X{lib$signal(SS$_DEBUG);
X return(v);}
X
X#endif
X
XLISP our_gc_mark(ptr)
X     LISP ptr;
X{return(NIL);}
X
Xvoid our_gc_free(ptr)
X     LISP ptr;
X{free(PNAME(ptr));
X PNAME(ptr) = 0;}
X
Xvoid our_print(ptr,f)
X     LISP ptr;
X     FILE *f;
X{fput_st(f,"\"");
X fput_st(f,PNAME(ptr));
X fput_st(f,"\"");}
X
X#define tc_string tc_user_1
X
XLISP strcons(length)
X     long length;
X{long flag;
X LISP s;
X s = symcons("",NIL);
X flag = no_interrupt(1);
X PNAME(s) = must_malloc(length);
X no_interrupt(flag);
X (*s).type = tc_string;
X return(s);}
X
XLISP string_append(args)
X     LISP args;
X{long size;
X LISP l,s;
X char *data;
X size = 0;
X for(l=args;NNULLP(l);l=cdr(l))
X   {s = car(l);
X    if (NTYPEP(s,tc_symbol) && NTYPEP(s,tc_string))
X      err("wta to string-append",s);
X    size = size + strlen(PNAME(s));}
X s = strcons(size+1);
X data = PNAME(s);
X data[0] = 0;
X for(l=args;NNULLP(l);l=cdr(l))
X   strcat(data,PNAME(car(l)));
X return(s);}
X
XLISP our_readm(tc,f)
X     int tc;
X     struct gen_readio *f;
X{char temp[100];
X int c;
X long j;
X LISP s;
X j = 0;
X while(((c = GETC_FCN(f)) != tc) && (c != EOF))
X   {if ((j + 2) > sizeof(temp)) err("read string overflow",NIL);
X    temp[j] = c;
X    ++j;}
X s = strcons(j+1);
X temp[j] = 0;
X strcpy(PNAME(s),temp);
X return(s);}
X
XLISP our_eval(obj,formp,envp)
X     LISP obj,*formp,*envp;
X{LISP ind;
X char buff[2];
X long n,j;
X if NTYPEP(obj,tc_string) err("eval bug",obj);
X n = strlen(PNAME(obj));
X ind = leval(car(cdr(*formp)),*envp);
X if NFLONUMP(ind) err("non numeric string index",ind);
X j = (long) FLONM(ind);
X if ((j < 0) || (j >= n)) err("string index out of range",ind);
X buff[0] = PNAME(obj)[j];
X buff[1] = 0;
X *formp = rintern(buff);
X return(NIL);}
X
Xint rfs_getc(p)
X     unsigned char **p;
X{int i;
X i = **p;
X if (!i) return(EOF);
X *p = *p + 1;
X return(i);}
X
Xvoid rfs_putc(c,p)
X     unsigned char c,**p;
X{*p = *p - 1;}
X
XLISP read_from_string(x)
X     LISP x;
X{char *p;
X if NTYPEP(x,tc_string) err("not a string",x);
X p = PNAME(x);
X return(gen_read(rfs_getc,rfs_putc,&p));}
X
Xour_subrs(flag)
X     int flag;
X{my_one = flocons((double) 1.0);
X my_two = flocons((double) 2.0);
X gc_protect(&my_one);
X gc_protect(&my_two);
X init_subr("cfib",tc_subr_1,cfib);
X#ifdef vms
X init_subr("edit",tc_subr_1,sys_edit);
X init_subr("vms-debug",tc_subr_1,vms_debug);
X#endif
X if (flag)
X   {init_subr("string-append",tc_lsubr,string_append);
X    init_subr("read-from-string",tc_subr_1,read_from_string);}}
