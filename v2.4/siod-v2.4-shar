XLISP gen_intern(name,copyp)
X     char *name;
X     long copyp;
X{LISP l,sym,sl;
X char *cname;
X long hash,n,c,flag;
X flag = no_interrupt(1);
X if (obarray_dim > 1)
X   {hash = 0;
X    n = obarray_dim;
X    cname = name;
X    while(c = *cname++) hash = ((hash * 17) ^ c) % n;
X    sl = obarray[hash];}
X else
X   sl = oblistvar;
X for(l=sl;NNULLP(l);l=CDR(l))
X   if (strcmp(name,PNAME(CAR(l))) == 0)
X     {no_interrupt(flag);
X      return(CAR(l));}
X if (copyp == 1)
X   {cname = must_malloc(strlen(name)+1);
X    strcpy(cname,name);}
X else
X   cname = name;
X sym = symcons(cname,unbound_marker);
X if (obarray_dim > 1) obarray[hash] = cons(sym,sl);
X oblistvar = cons(sym,oblistvar);
X no_interrupt(flag);
X return(sym);}
