XLISP vload(fname,cflag)
X     char *fname;
X     long cflag;
X{LISP form,result,tail;
X FILE *f;
X put_st("loading ");
X put_st(fname);
X put_st("\n");
X f = fopen_care(fname,"r");
X result = NIL;
X tail = NIL;
X while(1)
X   {form = lreadf(f);
X    if EQ(form,eof_val) break;
X    if (cflag)
X      {form = cons(form,NIL);
X       if NULLP(result)
X	 result = tail = form;
X       else
X	 tail = setcdr(tail,form);}
X    else
X      leval(form,NIL);}
X fclose_care(f);
X put_st("done.\n");
X return(result);}
