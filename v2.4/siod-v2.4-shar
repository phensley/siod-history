X(define (*cons-stream head tail-future)
X  (list head () () tail-future))
X
X(define head car)
X
X(define (tail x)
X  (if (car (cdr x))
X      (car (cdr (cdr x)))
X      (let ((value ((car (cdr (cdr (cdr x)))))))
X	(set-car! (cdr x) t)
X	(set-car! (cdr (cdr x)) value))))
X
X(defmac (cons-stream form)
X  (list '*cons-stream
X	(cadr form)
X	(list 'lambda () (caddr form))))
X
X(define (enumerate-interval low high)
X  (if (> low high)
X      the-empty-stream
X      (cons-stream low (enumerate-interval (+ low 1) high))))
X
X(define (print-stream-elements x)
X  (if (empty-stream? x)
X      ()
X      (begin (print (head x))
X	     (print-stream-elements (tail x)))))
X
X(define (sum-stream-elements x)
X  (define (loop acc x)
X    (if (empty-stream? x)
X	acc
X      (loop (+ (head x) acc) (tail x))))
X  (loop 0 x))
X
X(define (standard-fib x)
X  (if (< x 2)
X      x
X      (+ (standard-fib (- x 1))
X	 (standard-fib (- x 2)))))
X
X(define (make-list n)
X  (define l ())
X  (define j 0)
X  (define (accumulate-list)
X    (if (< j n)
X	(begin (setq l (cons () l))
X	       (setq j (+ j 1))
X	       (accumulate-list))))
X  (accumulate-list)
X  l)
